{
  "name": "rehype-prism-diff",
  "version": "1.1.1",
  "description": "rehype plugin to highlight diff block like github markdown with Prism",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules npx jest",
    "coverage": "cross-env NODE_OPTIONS=--experimental-vm-modules npx jest --coverage",
    "build": "tsc && rollup -c rollup.config.ts --configPlugin typescript",
    "prepare": "npm run build"
  },
  "author": "enpitsulin",
  "license": "MIT",
  "dependencies": {
    "hast-util-from-string": "^2.0.0",
    "hast-util-to-string": "^2.0.0",
    "unist-util-visit": "^4.1.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-typescript": "^8.3.2",
    "@types/dedent": "^0.7.0",
    "@types/hast": "^2.3.4",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.24",
    "cross-env": "^7.0.3",
    "dedent": "^0.7.0",
    "jest": "^27.5.1",
    "rehype": "^12.0.1",
    "rehype-prism": "^2.1.2",
    "rehype-prism-plus": "^1.3.2",
    "rollup": "^2.70.1",
    "ts-jest": "^27.1.4",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3",
    "unified": "^10.1.2"
  }
}
